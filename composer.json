{
  "name": "ajur-media/php-image-resize",
  "description": "PHP class to resize and scale images",
  "keywords": [
    "php",
    "image",
    "resize",
    "scale"
  ],
  "type": "library",
  "homepage": "https://github.com/ajur-media/php-image-resize",
  "license": "MIT",
  "authors": [
    {
      "name": "Karel Wintersky",
      "homepage": "https://karel.wintersky.ru"
    }
  ],
  "require": {
    "php": "^7.4 | ^8.0",
    "ext-gd": "*",
    "ext-fileinfo": "*",
    "ext-exif": "*",
    "ext-json": "*"
  },
  "suggest": {
    "ext-exif": "Auto-rotate jpeg files"
  },
  "autoload": {
    "psr-4": {
      "AJUR\\Toolkit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AJUR\\Toolkit\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5 | ^9.5",
    "apigen/apigen": "^4.1",
    "php-coveralls/php-coveralls": "^2.1",
    "rector/rector": "^1.2"
  }
}
